{
  "version": 2,
  "projects": {
    "demo": {
      "root": "packages/demo",
      "sourceRoot": "packages/demo",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "packages/demo",
            "outputPath": "dist/packages/demo"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "demo:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "demo:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "demo:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/demo"],
          "options": {
            "jestConfig": "packages/demo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/demo/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "demo-e2e": {
      "root": "packages/demo-e2e",
      "sourceRoot": "packages/demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/demo-e2e/cypress.json",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/demo-e2e/**/*.{js,ts}"]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "e2e-shared-components-bg-button-e2e": "packages/e2e/shared/components/bg-button-e2e",
    "e2e-shared-components-test-e2e": "packages/e2e/shared/components/test-e2e",
    "e2e-shared-components-test10-e2e": "packages/e2e/shared/components/test10-e2e",
    "e2e-shared-components-test2-e2e": "packages/e2e/shared/components/test2-e2e",
    "e2e-shared-components-test3-e2e": "packages/e2e/shared/components/test3-e2e",
    "e2e-shared-components-test4-e2e": "packages/e2e/shared/components/test4-e2e",
    "e2e-shared-components-test5-e2e": "packages/e2e/shared/components/test5-e2e",
    "e2e-shared-components-test6-e2e": "packages/e2e/shared/components/test6-e2e",
    "e2e-shared-components-test7-e2e": "packages/e2e/shared/components/test7-e2e",
    "e2e-shared-components-test8-e2e": "packages/e2e/shared/components/test8-e2e",
    "e2e-shared-components-test9-e2e": "packages/e2e/shared/components/test9-e2e",
    "shared-bg-button": {
      "root": "packages/shared/bg-button",
      "sourceRoot": "packages/shared/bg-button/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/shared/bg-button",
            "tsConfig": "packages/shared/bg-button/tsconfig.lib.json",
            "project": "packages/shared/bg-button/package.json",
            "entryFile": "packages/shared/bg-button/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/shared/bg-button/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "packages/shared/bg-button/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/shared/bg-button"],
          "options": {
            "jestConfig": "packages/shared/bg-button/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        },
        "deploy": {
          "executor": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public"
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "packages/shared/bg-button/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shared-bg-button",
            "config": {
              "configFolder": "packages/shared/bg-button/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "npm-publish": {
          "executor": "./tools/executors/npm-publish:npm-publish",
          "options": {
            "textToEcho": "Hello World"
          }
        }
      }
    },
    "shared-components-test": "packages/shared/components/test",
    "shared-components-test10": "packages/shared/components/test10",
    "shared-components-test2": "packages/shared/components/test2",
    "shared-components-test3": "packages/shared/components/test3",
    "shared-components-test4": "packages/shared/components/test4",
    "shared-components-test5": "packages/shared/components/test5",
    "shared-components-test6": "packages/shared/components/test6",
    "shared-components-test7": "packages/shared/components/test7",
    "shared-components-test8": "packages/shared/components/test8",
    "shared-components-test9": "packages/shared/components/test9"
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  }
}
